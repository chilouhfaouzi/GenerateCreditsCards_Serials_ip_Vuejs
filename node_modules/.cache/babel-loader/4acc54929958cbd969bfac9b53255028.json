sContent":["<template>\r\n  <div class=\"serial\">\r\n    <div class=\"container\">\r\n      <h1 class=\"text-center\">Serials Generator</h1>\r\n      <div class=\"row\">\r\n        <div class=\"col-md-6\">\r\n          <div class=\"input\">\r\n            <div class=\"form-group\">\r\n              <label for=\"serialLen\">Serials Length</label>\r\n              <input\r\n                v-model=\"serialLen\"\r\n                type=\"text\"\r\n                class=\"form-control\"\r\n                id=\"serialLen\"\r\n                placeholder=\"xx\"\r\n                maxlength=\"2\"\r\n                size=\"2\"\r\n              />\r\n              <label for=\"number\">Numbers of serials</label>\r\n              <input\r\n                v-model=\"numberserials\"\r\n                type=\"text\"\r\n                min=\"5\"\r\n                class=\"form-control\"\r\n                id=\"number\"\r\n                placeholder=\"xx\"\r\n                maxlength=\"2\"\r\n                size=\"2\"\r\n              />\r\n              <button @click=\"generer\" class=\"mt-3 btn btn-dark\">\r\n                <img src=\"../assets/set.svg\" /> Generate Serials\r\n              </button>\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <div class=\"col-md-6\">\r\n          <div class=\"output\">\r\n            <div class=\"form-group\">\r\n              <textarea\r\n                v-model=\"all_serials\"\r\n                class=\"form-control\"\r\n                rows=\"11\"\r\n                cols=\"30\"\r\n                readonly\r\n              ></textarea>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"serial\",\r\n  data() {\r\n    return {\r\n      serialLen: 20,\r\n      numberserials: 10,\r\n      all_serials: \"\",\r\n      chars: \"1234567890ABCDEFGHJIKLMNORQPSTVUYZXabcdefghjkilmnoprqstvuzyx\",\r\n    };\r\n  },\r\n  methods: {\r\n    generer() {\r\n      this.all_serials = \"\";\r\n      for (let j = 0; j < this.numberserials; j++) {\r\n        let serial = \"\";\r\n        for (let i = 0; i < this.serialLen; i++) {\r\n          if (i % 4 == 0 && i > 0) {\r\n            serial += \"-\";\r\n          }\r\n          var randmKey = Math.floor(Math.random() * this.chars.length);\r\n\r\n          serial += this.chars[randmKey];\r\n        }\r\n        this.all_serials += serial;\r\n        this.all_serials += \"\\n\";\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.serial {\r\n  padding: 30px 0;\r\n}\r\n.serial img {\r\n  width: 25px;\r\n  margin-right: 8px;\r\n}\r\n</style>\r\n"],"sourceRoot":""}]}