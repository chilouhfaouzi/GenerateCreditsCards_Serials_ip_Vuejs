,CAYI,CAzBN,CAsCI,EAXJ,WAWI,E,gBAVJ,YAAA,CASE,OATF,EASE;;aARS,KAAA,CAAA,GAAA,GAAG,M;MAQZ;AAPA,IAAA,IAAI,EAAC,MAOL;AANA,IAAA,KAAK,EAAC,cAMN;AALA,IAAA,IAAI,EAAC,KAKL;AAJA,IAAA,EAAE,EAAC,KAIH;AAHA,IAAA,WAAW,EAAC,KAGZ;AAFA,IAAA,SAAS,EAAC,GAEV;AADA,IAAA,IAAI,EAAC;AACL,GATF,E,IAAA,E;;AAAA,G,iBACW,KAAA,CAAA,G,GASP,CAjDN,CAqDI,EAHJ,YAAA,CAES,QAFT,EAES;AAFA,IAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA;AAAA,aAAE,QAAA,CAAA,MAAA,OAAA,QAAA,YAAF;AAAA,KAAA,CAEL;AAFe,IAAA,KAAK,EAAC;AAErB,GAFT,E,CACE,W,cADF,CAGI,CAtDN,CAuDI,CAxDN,CAsEI,EAbJ,YAAA,CAYM,KAZN,EAAA,WAAA,EAYM,CAXJ,YAAA,CAUM,KAVN,EAAA,WAAA,EAUM,CATJ,YAAA,CAQM,KARN,EAAA,WAAA,EAQM,C,gBAPJ,YAAA,CAMY,UANZ,EAMY;;aALD,KAAA,CAAA,UAAA,GAAU,M;MAKT;AAJV,IAAA,KAAK,EAAC,cAII;AAHV,IAAA,IAAI,EAAC,IAGK;AAFV,IAAA,IAAI,EAAC,IAEK;AADV,IAAA,QAAQ,EAAR;AACU,GANZ,E,IAAA,E;;AAAA,G,iBACW,KAAA,CAAA,U,GAMP,CARN,CASI,CAVN,CAWI,CAZN,CAaI,CAvEN,CAwEI,CA1EN,CA2EI,CA5EN,C","sourcesContent":["<template>\r\n  <div class=\"credit\">\r\n    <div class=\"container\">\r\n      <h1 class=\"text-center\">Credit card Generator</h1>\r\n      <div class=\"row\">\r\n        <div class=\"col-md-6\">\r\n          <div class=\"input\">\r\n            <div class=\"form-group\">\r\n              <label for=\"gener\">BIN </label>\r\n              <input\r\n                type=\"text\"\r\n                class=\"form-control\"\r\n                id=\"gener\"\r\n                :value=\"\r\n                  credit_number == 556273 ? '556273xxxxxxxxxxx' : credit_number\r\n                \"\r\n                placeholder=\"5562 73xxx xxxx xxxx\"\r\n              />\r\n              <div class=\"row\">\r\n                <div class=\"col\">\r\n                  <label for=\"month\">Month </label>\r\n                  <input\r\n                    v-model=\"month\"\r\n                    type=\"text\"\r\n                    class=\"form-control\"\r\n                    id=\"month\"\r\n                    placeholder=\"xx\"\r\n                    maxlength=\"2\"\r\n                    size=\"2\"\r\n                  />\r\n                </div>\r\n                <div class=\"col\">\r\n                  <label for=\"year\">Year </label>\r\n                  <input\r\n                    v-model=\"year\"\r\n                    type=\"text\"\r\n                    class=\"form-control\"\r\n                    id=\"year\"\r\n                    placeholder=\"xx\"\r\n                    maxlength=\"2\"\r\n                    size=\"2\"\r\n                  />\r\n                </div>\r\n              </div>\r\n\r\n              <label for=\"rdv\">CCV </label>\r\n              <input\r\n                v-model=\"rdv\"\r\n                type=\"text\"\r\n                class=\"form-control\"\r\n                name=\"rdv\"\r\n                id=\"rdv\"\r\n                placeholder=\"xxx\"\r\n                maxlength=\"3\"\r\n                size=\"3\"\r\n              />\r\n            </div>\r\n            <button @click=\"genere\" class=\"btn btn-dark\">\r\n              <img src=\"../assets/set.svg\" /> Generate Cards\r\n            </button>\r\n          </div>\r\n        </div>\r\n        <div class=\"col-md-6\">\r\n          <div class=\"output\">\r\n            <div class=\"form-group\">\r\n              <textarea\r\n                v-model=\"all_credit\"\r\n                class=\"form-control\"\r\n                rows=\"11\"\r\n                cols=\"30\"\r\n                readonly\r\n              ></textarea>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"credit\",\r\n  data() {\r\n    return {\r\n      all_credit: \"\",\r\n      credit_number: \"556273\",\r\n      month: \"\",\r\n      year: \"\",\r\n      rdv: \"\",\r\n    };\r\n  },\r\n\r\n  methods: {\r\n    genere() {\r\n      var valueStr = document.getElementById(\"gener\").value;\r\n      valueStr = valueStr.replace(/x/g, \"\");\r\n      this.credit_number = valueStr.replace(/\\s/g, \"\");\r\n      var ancien = this.credit_number;\r\n      this.all_credit = \"\";\r\n\r\n      for (let i = 0; i < 10; i++) {\r\n        while (this.credit_number.length < 16) {\r\n          this.credit_number += Math.floor(Math.random() * 10);\r\n        }\r\n        this.credit_number += \"|\";\r\n        if (this.month == \"\") {\r\n          var testmounth = 0;\r\n\r\n          this.month = Math.floor(Math.random() * 12) + 1;\r\n          if (this.month < 10) {\r\n            this.month = \"0\" + this.month;\r\n          }\r\n        }\r\n        this.credit_number += this.month;\r\n        if (testmounth == 0) {\r\n          this.month = \"\";\r\n        }\r\n\r\n        if (this.year == \"\") {\r\n          var testyear = 0;\r\n          this.year = Math.floor(Math.random() * 6) + 20;\r\n        }\r\n        this.credit_number += \"-\";\r\n        this.credit_number += this.year;\r\n        if (testyear == 0) {\r\n          this.year = \"\";\r\n        }\r\n\r\n        this.credit_number += \"|\";\r\n\r\n        if (this.rdv == \"\") {\r\n          var testrdv = 0;\r\n\r\n          for (let i = 0; i < 3; i++) {\r\n            this.rdv += Math.floor(Math.random() * 10);\r\n          }\r\n        }\r\n        this.credit_number += this.rdv;\r\n        if (testrdv == 0) {\r\n          this.rdv = \"\";\r\n        }\r\n        this.all_credit += this.credit_number;\r\n        this.all_credit += \"\\n\";\r\n        this.credit_number = ancien;\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.credit {\r\n  padding: 30px 0;\r\n}\r\n.credit img {\r\n  width: 25px;\r\n  margin-right: 8px;\r\n}\r\n.output {\r\n  margin-top: 25px;\r\n}\r\n</style>\r\n"],"sourceRoot":""}]}